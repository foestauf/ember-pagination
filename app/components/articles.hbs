<h2 id="me">Posts</h2>
<div>
    <ul>
        {{#each this.pagedContent as |post|}}
        <Article @route="posts.post" @model={{post}} />
        <li>
            <User />User: {{post.user.name}} Title: {{post.title}}</li>
        {{/each}}
    </ul>
</div>

<PageNumbers @content={{this.pagedContent}} />